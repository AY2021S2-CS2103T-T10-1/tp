@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":PawbookParser" as PawbookParser LOGIC_COLOR
participant ":DeleteOwnerCommandParser" as DeleteOwnerCommandParser LOGIC_COLOR
participant "d:DeleteOwnerCommand" as DeleteOwnerCommand LOGIC_COLOR
participant ":CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("delete owner 1")
activate LogicManager

LogicManager -> PawbookParser : parseCommand("delete owner 1")
activate PawbookParser

create DeleteOwnerCommandParser
PawbookParser -> DeleteOwnerCommandParser
activate DeleteOwnerCommandParser

DeleteOwnerCommandParser --> PawbookParser
deactivate DeleteOwnerCommandParser

PawbookParser -> DeleteOwnerCommandParser : parse("1")
activate DeleteOwnerCommandParser

create DeleteOwnerCommand
DeleteOwnerCommandParser -> DeleteOwnerCommand
activate DeleteOwnerCommand

DeleteOwnerCommand --> DeleteOwnerCommandParser : d
deactivate DeleteOwnerCommand

DeleteOwnerCommandParser --> PawbookParser : d
deactivate DeleteOwnerCommandParser
'Hidden arrow to position the destroy marker below the end of the activation bar.
DeleteOwnerCommandParser -[hidden]-> PawbookParser
destroy DeleteOwnerCommandParser

PawbookParser --> LogicManager : d
deactivate PawbookParser

LogicManager -> DeleteOwnerCommand : execute()
activate DeleteOwnerCommand

DeleteOwnerCommand -> Model : deleteEntity(1)
activate Model

Model --> DeleteOwnerCommand
deactivate Model

create CommandResult
DeleteOwnerCommand -> CommandResult
activate CommandResult

CommandResult --> DeleteOwnerCommand
deactivate CommandResult

DeleteOwnerCommand --> LogicManager : result
deactivate DeleteOwnerCommand

[<--LogicManager
deactivate LogicManager
@enduml
