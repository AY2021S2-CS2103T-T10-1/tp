@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model <<Rectangle>>{
Interface ReadOnlyDatabase <<Interface>>
Interface Model <<Interface>>
Interface ObservableList <<Interface>>
Class Database
Class ReadOnlyDatabase
Class Model
Class ModelManager
Class UserPrefs
Class ReadOnlyUserPrefs

Package "Managed Entity" {
Class UniqueEntityList
abstract Class Entity
Class Name


Package Owner {
Class Owner
Class Address
Class Email
Class Phone
}

Package Dog {
Class Dog
Class Breed
Class DateOfBirth
Class Sex
}

Package Program {
Class Program
Class Session
}

Package Tag {
Class Tag
}

}
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

Database .up.|> ReadOnlyDatabase

ModelManager .up.|> Model
Model .right.> ObservableList
ModelManager o--> "1" Database
ModelManager o-left-> "1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs

Database *-> "1" UniqueEntityList
UniqueEntityList o--> "*" Entity
Entity ^--- Program
Entity ^--- Owner
Entity ^--- Dog

Entity *-> Name
Entity *--> "*" Tag

Owner *--> Phone
Owner *--> Email
Owner *--> Address

Dog *--> Breed
Dog *--> DateOfBirth
Dog *--> Sex

Program *--> "*" Session

Tag -[hidden]right-> Name
Phone -[hidden]right-> Address
Address -[hidden]right-> Email

Sex -[hidden]right-> Breed
Breed -[hidden]right-> DateOfBirth

ModelManager ------>"~* filtered list" Entity
@enduml
